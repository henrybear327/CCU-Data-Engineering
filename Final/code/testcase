Infix to postfix

make && ./nfa -pat "a?" -str "b"
make && ./nfa -pat "(a?b)|c" -str "b"
make && ./nfa -pat "(a?b)|c|d" -str "b"
make && ./nfa -pat "abcdef" -str "b"
make && ./nfa -pat "(ab)+(cd)?" -str "b"
make && ./nfa -pat "((ab)+|(cd)?)a" -str "b"
make && ./nfa -pat "abc(a?b*c+)|(ccc+)|(c+a*a?)cba" -str "b"
make && ./nfa -pat "(a?b)|c|d" -str "b"
 